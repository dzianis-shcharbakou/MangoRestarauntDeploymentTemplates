{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    },
    "vaultName": {
      "type": "object"
    },
    "keyData": {
      "type": "array"
    },
    "sqlOutput": {
      "type": "object"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.KeyVault/vaults/secrets",
      "apiVersion": "2022-07-01",
      "name": "[concat(parameters('vaultName'), '/', parameters('keyData')[copyIndex()].key)]",
      "location": "[parameters('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Resources/deployments', 'sqlDb')]"
      ],
      "copy": {
          "name": "keyValueCopy",
          "count": "[length(parameters('keyData'))]"
      },
      "properties": {
        "contentType": "securestring",
        "value": "[parameters('sqlOutput')[parameters('keyData')[copyIndex()].value].value]",
        "attributes": {
          "enabled": true
        }
      }
    }
  ]
}

